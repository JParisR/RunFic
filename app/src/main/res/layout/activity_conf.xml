<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".module.conf.ConfActivity">

    <TextView
        android:id="@+id/text_subt"
        android:text="@string/versubtitle_sett"
        android:layout_width="200dp"
        android:layout_height="25dp"
         />

    <CheckBox
        android:id="@+id/cb_subt"
        android:layout_marginLeft="5dp"
        android:layout_toRightOf="@id/text_subt"
        android:layout_width="50dp"
        android:layout_alignParentRight="true"
        android:layout_height="25dp"
        />

    <TextView
        android:id="@+id/text_desc"
        android:text="@string/verdescription_sett"
        android:layout_width="200dp"
        android:layout_height="25dp"
        android:layout_below="@id/text_subt"
        />

    <CheckBox
        android:layout_marginLeft="5dp"
        android:layout_alignParentRight="true"
        android:layout_toRightOf="@id/text_desc"
        android:layout_below="@id/cb_subt"
        android:id="@+id/cb_desc"
        android:layout_width="50dp"
        android:layout_height="25dp"
        />

    </RelativeLayout>
